        }
        screeningItemsToConsiderDto.setSeniorScreenerSeniorAttributeValue( user.getSeniorScreenerSeniorAttributeValue() );
        if ( user.getShareholderAbilityProvsFlg() != null ) {
            screeningItemsToConsiderDto.setShareholderAbilityProvsFlg( Integer.parseInt( user.getShareholderAbilityProvsFlg() ) );
        }
        screeningItemsToConsiderDto.setShareholderAbilityProvsNt( user.getShareholderAbilityProvsNt() );
        screeningItemsToConsiderDto.setTcrLastDateChecked( user.getTcrLastDateChecked() );
        screeningItemsToConsiderDto.setTcrNote( user.getTcrNote() );
        if ( user.getTcrNoteFlg() != null ) {
            screeningItemsToConsiderDto.setTcrNoteFlg( Integer.parseInt( user.getTcrNoteFlg() ) );
        }
        screeningItemsToConsiderDto.setTierRegA( user.getTierRegA() );
        screeningItemsToConsiderDto.setUniqueTransNt( user.getUniqueTransNt() );
        if ( user.getUniqueTransNtFlg() != null ) {
            screeningItemsToConsiderDto.setUniqueTransNtFlg( Integer.parseInt( user.getUniqueTransNtFlg() ) );
        }

        return screeningItemsToConsiderDto;
    }

