    @Override
    public List<ScreenDfDataDto> mapToScreenDfDataDto(List<ScreenDfData> screenDfData) {
        if ( screenDfData == null ) {
            return null;
        }

        List<ScreenDfDataDto> list = new ArrayList<ScreenDfDataDto>( screenDfData.size() );
        for ( ScreenDfData screenDfData1 : screenDfData ) {
            list.add( screenDfDataToScreenDfDataDto( screenDfData1 ) );
        }

        return list;
    }

    @Override
    public List<ScreeningDynamicFieldsDataDto> mapToScreenDynamicDataDto1(List<ScreenDynamicField> screenDfData) {
        if ( screenDfData == null ) {
            return null;
        }

        List<ScreeningDynamicFieldsDataDto> list = new ArrayList<ScreeningDynamicFieldsDataDto>( screenDfData.size() );
        for ( ScreenDynamicField screenDynamicField : screenDfData ) {
            list.add( screenDynamicFieldToScreeningDynamicFieldsDataDto( screenDynamicField ) );
        }

        return list;
    }

    @Override
    public List<ScreeningCaseDto> mapToScreenCaseDto(List<ScreenCase> screenCase) {
        if ( screenCase == null ) {
            return null;
        }

        List<ScreeningCaseDto> list = new ArrayList<ScreeningCaseDto>( screenCase.size() );
        for ( ScreenCase screenCase1 : screenCase ) {
            list.add( mapToScreenCaseDto( screenCase1 ) );
        }

        return list;
    }

    @Override
    public ScreenCase mapToScreenCase(ScreeningCaseDto screenCase) {
        if ( screenCase == null ) {
            return null;
        }

        ScreenCase screenCase1 = new ScreenCase();

        screenCase1.setScreening( maptoScreening( screenCase.getScreeningId() ) );
        screenCase1.setCreatedBy( screenCase.getCreatedBy() );
        try {
            if ( screenCase.getCreatedDt() != null ) {
                screenCase1.setCreatedDt( new SimpleDateFormat().parse( screenCase.getCreatedDt() ) );
            }
        }
        catch ( ParseException e ) {
            throw new RuntimeException( e );
        }
        screenCase1.setUpdatedBy( screenCase.getUpdatedBy() );
        try {
            if ( screenCase.getUpdatedDt() != null ) {
                screenCase1.setUpdatedDt( new SimpleDateFormat().parse( screenCase.getUpdatedDt() ) );
            }
        }
        catch ( ParseException e ) {
            throw new RuntimeException( e );
        }
        if ( screenCase.getScreenCaseId() != null ) {
            screenCase1.setScreenCaseId( screenCase.getScreenCaseId() );
        }
        screenCase1.setBackupCfContact( screenCase.getBackupCfContact() );
        screenCase1.setCaseNumber( screenCase.getCaseNumber() );
        if ( screenCase.getCfMstReviewId() != null ) {
            screenCase1.setCfMstReviewId( screenCase.getCfMstReviewId().longValue() );
        }
        screenCase1.setDeleted( screenCase.getDeleted() );
        screenCase1.setEnfAd( screenCase.getEnfAd() );
        screenCase1.setEnfAdEmail( screenCase.getEnfAdEmail() );
        screenCase1.setEnfAttorney( screenCase.getEnfAttorney() );
        screenCase1.setEnfAttorneyEmail( screenCase.getEnfAttorneyEmail() );
        screenCase1.setIpnDate( screenCase.getIpnDate() );
        screenCase1.setIpnDoc( screenCase.getIpnDoc() );
        screenCase1.setIpnGenerated( screenCase.getIpnGenerated() );
        if ( screenCase.getReviewRound() != null ) {
            screenCase1.setReviewRound( BigDecimal.valueOf( screenCase.getReviewRound() ) );
        }
        screenCase1.setScreener( screenCase.getScreener() );

        return screenCase1;
    }

    @Override
    public RvwAuditInfo mapToRvwAudInfo(RvwAuditInfoDto rvwAudit) {
        if ( rvwAudit == null ) {
            return null;
        }

        RvwAuditInfo rvwAuditInfo = new RvwAuditInfo();

        rvwAuditInfo.setRvwAuditInfoPK( rvwAuditInfoDtoToRvwAuditInfoPK( rvwAudit ) );
        rvwAuditInfo.setAuditInfoNote( rvwAudit.getAuditInfoNote() );
        rvwAuditInfo.setAuditedDt( rvwAudit.getAuditedDt() );
        rvwAuditInfo.setAuditor( rvwAudit.getAuditor() );
        rvwAuditInfo.setConsentDt( rvwAudit.getConsentDt() );
        rvwAuditInfo.setFsType( rvwAudit.getFsType() );
        rvwAuditInfo.setFsTypeOth( rvwAudit.getFsTypeOth() );
        rvwAuditInfo.setFsUpdated( rvwAudit.getFsUpdated() );
        rvwAuditInfo.setFsUpdatedDueDt( rvwAudit.getFsUpdatedDueDt() );
        rvwAuditInfo.setLatestUnauditedDt( rvwAudit.getLatestUnauditedDt() );
        rvwAuditInfo.setLocation( rvwAudit.getLocation() );
        rvwAuditInfo.setPcaob( rvwAudit.getPcaob() );
        rvwAuditInfo.setReportDt( rvwAudit.getReportDt() );

        return rvwAuditInfo;
    }

    @Override
    public ScreeningCaseDto mapToScreenCaseDto(ScreenCase saveScreeningCase) {
        if ( saveScreeningCase == null ) {
            return null;
        }

        ScreeningCaseDto screeningCaseDto = new ScreeningCaseDto();

        screeningCaseDto.setBackupCfContact( saveScreeningCase.getBackupCfContact() );
        screeningCaseDto.setCaseNumber( saveScreeningCase.getCaseNumber() );
        if ( saveScreeningCase.getCfMstReviewId() != null ) {
            screeningCaseDto.setCfMstReviewId( saveScreeningCase.getCfMstReviewId().intValue() );
        }
        screeningCaseDto.setCreatedBy( saveScreeningCase.getCreatedBy() );
        if ( saveScreeningCase.getCreatedDt() != null ) {
            screeningCaseDto.setCreatedDt( new SimpleDateFormat().format( saveScreeningCase.getCreatedDt() ) );
        }
        screeningCaseDto.setDeleted( saveScreeningCase.getDeleted() );
        screeningCaseDto.setEnfAd( saveScreeningCase.getEnfAd() );
        screeningCaseDto.setEnfAdEmail( saveScreeningCase.getEnfAdEmail() );
        screeningCaseDto.setEnfAttorney( saveScreeningCase.getEnfAttorney() );
        screeningCaseDto.setEnfAttorneyEmail( saveScreeningCase.getEnfAttorneyEmail() );
        screeningCaseDto.setIpnDate( saveScreeningCase.getIpnDate() );
        screeningCaseDto.setIpnDoc( saveScreeningCase.getIpnDoc() );
        screeningCaseDto.setIpnGenerated( saveScreeningCase.getIpnGenerated() );
        if ( saveScreeningCase.getReviewRound() != null ) {
            screeningCaseDto.setReviewRound( saveScreeningCase.getReviewRound().intValue() );
        }
        screeningCaseDto.setScreenCaseId( (int) saveScreeningCase.getScreenCaseId() );
        screeningCaseDto.setScreener( saveScreeningCase.getScreener() );
        screeningCaseDto.setUpdatedBy( saveScreeningCase.getUpdatedBy() );
        if ( saveScreeningCase.getUpdatedDt() != null ) {
            screeningCaseDto.setUpdatedDt( new SimpleDateFormat().format( saveScreeningCase.getUpdatedDt() ) );
        }

        return screeningCaseDto;
    }

    @Override
    public Screening maptoScreening(Integer screeningId) {
        if ( screeningId == null ) {
            return null;
        }

        Screening screening = new Screening();

        if ( screeningId != null ) {
            screening.setScreeningId( screeningId );
        }

        return screening;
    }

    @Override
    public List<WorkspaceReviewPermFilesDto> maptoPermFileDto(List<PermFileShort> permFileItems) {
        if ( permFileItems == null ) {
            return null;
        }

        List<WorkspaceReviewPermFilesDto> list = new ArrayList<WorkspaceReviewPermFilesDto>( permFileItems.size() );
        for ( PermFileShort permFileShort : permFileItems ) {
            list.add( permFileShortToWorkspaceReviewPermFilesDto( permFileShort ) );
        }

        return list;
    }

    @Override
    public RvwPreliminaryStepsWrksp mapToPreliminaryStp(RvwPreliminaryStepsWrkspDto rvwPreliminarySteptoadd) {
        if ( rvwPreliminarySteptoadd == null ) {
            return null;
        }

        RvwPreliminaryStepsWrksp rvwPreliminaryStepsWrksp = new RvwPreliminaryStepsWrksp();

        rvwPreliminaryStepsWrksp.setId( rvwPreliminaryStepsWrkspDtoToRvwPreliminaryStepsWrkspPK( rvwPreliminarySteptoadd ) );
        if ( rvwPreliminarySteptoadd.getDelayAmendMissing() != null ) {
            rvwPreliminaryStepsWrksp.setDelayAmendMissing( String.valueOf( rvwPreliminarySteptoadd.getDelayAmendMissing() ) );
        }
        rvwPreliminaryStepsWrksp.setIssuerElegibility( rvwPreliminarySteptoadd.getIssuerElegibility() );
        if ( rvwPreliminarySteptoadd.getNewCtr() != null ) {
            rvwPreliminaryStepsWrksp.setNewCtr( String.valueOf( rvwPreliminarySteptoadd.getNewCtr() ) );
        }
        if ( rvwPreliminarySteptoadd.getNewReferral() != null ) {
            rvwPreliminaryStepsWrksp.setNewReferral( String.valueOf( rvwPreliminarySteptoadd.getNewReferral() ) );
        }
        rvwPreliminaryStepsWrksp.setOfferingElegibility( rvwPreliminarySteptoadd.getOfferingElegibility() );
        rvwPreliminaryStepsWrksp.setSecurityElegibility( rvwPreliminarySteptoadd.getSecurityElegibility() );
        rvwPreliminaryStepsWrksp.setStlIncmpltFnanclStmt( rvwPreliminarySteptoadd.getStlIncmpltFnanclStmt() );
        rvwPreliminaryStepsWrksp.setUniqueTrnsOthFct( rvwPreliminarySteptoadd.getUniqueTrnsOthFct() );

        return rvwPreliminaryStepsWrksp;
    }

    @Override
    public ScreenDrsRegistrationDto mapToScreenDrsRegDto(ScreenDrsRegistration drs) {
        if ( drs == null ) {
            return null;
        }

        ScreenDrsRegistrationDto screenDrsRegistrationDto = new ScreenDrsRegistrationDto();

        screenDrsRegistrationDto.setCfMstReviewId( drs.getCfMstReviewId() );
        screenDrsRegistrationDto.setFollowMktCapBsdFlgDt( drs.getFollowMktCapBsdFlgDt() );
        screenDrsRegistrationDto.setIpoAnnualRevenue( drs.getIpoAnnualRevenue() );
        screenDrsRegistrationDto.setReviewRound( drs.getReviewRound() );
        screenDrsRegistrationDto.setSellingShareHolder( drs.getSellingShareHolder() );

        return screenDrsRegistrationDto;
    }

    @Override
    public ScreenDrsRegistration mapToScreenDrsReg(ScreenDrsRegistrationDto drs) {
        if ( drs == null ) {
            return null;
        }

        ScreenDrsRegistration screenDrsRegistration = new ScreenDrsRegistration();

        screenDrsRegistration.setCfMstReviewId( drs.getCfMstReviewId() );
        screenDrsRegistration.setFollowMktCapBsdFlgDt( drs.getFollowMktCapBsdFlgDt() );
        screenDrsRegistration.setIpoAnnualRevenue( drs.getIpoAnnualRevenue() );
        screenDrsRegistration.setReviewRound( drs.getReviewRound() );
        screenDrsRegistration.setSellingShareHolder( drs.getSellingShareHolder() );

        return screenDrsRegistration;
    }

    private String xmlGregorianCalendarToString( XMLGregorianCalendar xcal, String dateFormat ) {
        if ( xcal == null ) {
            return null;
        }

        if (dateFormat == null ) {
            return xcal.toString();
        }
        else {
            Date d = xcal.toGregorianCalendar().getTime();
            SimpleDateFormat sdf = new SimpleDateFormat( dateFormat );
            return sdf.format( d );
        }
    }

    private XMLGregorianCalendar dateToXmlGregorianCalendar( Date date ) {
        if ( date == null ) {
            return null;
        }

        GregorianCalendar c = new GregorianCalendar();
        c.setTime( date );
        return datatypeFactory.newXMLGregorianCalendar( c );
    }

    private Long userScreeningScreeningId(ScreenRegistration screenRegistration) {
        Screening screening = screenRegistration.getScreening();
        if ( screening == null ) {
            return null;
        }
        return screening.getScreeningId();
    }

    protected ScreenDfDataDto screenDfDataToScreenDfDataDto(ScreenDfData screenDfData) {
        if ( screenDfData == null ) {
            return null;
        }

        ScreenDfDataDto screenDfDataDto = new ScreenDfDataDto();

        screenDfDataDto.setFScreenValue( screenDfData.getFScreenValue() );
        screenDfDataDto.setIScreenValue( screenDfData.getIScreenValue() );
        screenDfDataDto.setIsActive( screenDfData.getIsActive() );
        screenDfDataDto.setLabelOrderBy( screenDfData.getLabelOrderBy() );
        screenDfDataDto.setScreenValueNote( screenDfData.getScreenValueNote() );
        screenDfDataDto.setScreenValueNoteFlg( screenDfData.getScreenValueNoteFlg() );
        screenDfDataDto.setScreeningId( screenDfData.getScreeningId() );
        screenDfDataDto.setSddId( screenDfData.getSddId() );

        return screenDfDataDto;
    }

    protected ScreeningDynamicFieldsDataDto screenDynamicFieldToScreeningDynamicFieldsDataDto(ScreenDynamicField screenDynamicField) {
        if ( screenDynamicField == null ) {
            return null;
        }

        ScreeningDynamicFieldsDataDto screeningDynamicFieldsDataDto = new ScreeningDynamicFieldsDataDto();

        screeningDynamicFieldsDataDto.setCreatedBy( screenDynamicField.getCreatedBy() );
        if ( screenDynamicField.getCreatedDt() != null ) {
            screeningDynamicFieldsDataDto.setCreatedDt( new SimpleDateFormat().format( screenDynamicField.getCreatedDt() ) );
        }
        if ( screenDynamicField.getIsActive() != null ) {
            screeningDynamicFieldsDataDto.setIsActive( Integer.parseInt( screenDynamicField.getIsActive() ) );
        }
        screeningDynamicFieldsDataDto.setLabelDisplayName( screenDynamicField.getLabelDisplayName() );
        if ( screenDynamicField.getLabelOrderBy() != null ) {
            screeningDynamicFieldsDataDto.setLabelOrderBy( screenDynamicField.getLabelOrderBy().intValue() );
        }
        screeningDynamicFieldsDataDto.setSdfId( (int) screenDynamicField.getSdfId() );
        screeningDynamicFieldsDataDto.setUpdatedBy( screenDynamicField.getUpdatedBy() );
        if ( screenDynamicField.getUpdatedDt() != null ) {
            screeningDynamicFieldsDataDto.setUpdatedDt( new SimpleDateFormat().format( screenDynamicField.getUpdatedDt() ) );
        }

        return screeningDynamicFieldsDataDto;
    }

    protected RvwAuditInfoPK rvwAuditInfoDtoToRvwAuditInfoPK(RvwAuditInfoDto rvwAuditInfoDto) {
        if ( rvwAuditInfoDto == null ) {
            return null;
        }

        RvwAuditInfoPK rvwAuditInfoPK = new RvwAuditInfoPK();

        rvwAuditInfoPK.setCfMstReviewId( rvwAuditInfoDto.getCfMstReviewId() );
        rvwAuditInfoPK.setRvwProfessionId( rvwAuditInfoDto.getRvwProfessionId() );
        rvwAuditInfoPK.setReviewRound( rvwAuditInfoDto.getReviewRound() );

        return rvwAuditInfoPK;
    }

    protected WorkspaceReviewPermFilesDto permFileShortToWorkspaceReviewPermFilesDto(PermFileShort permFileShort) {
        if ( permFileShort == null ) {
            return null;
        }

        WorkspaceReviewPermFilesDto workspaceReviewPermFilesDto = new WorkspaceReviewPermFilesDto();

        if ( permFileShort.getCfMstReviewId() != null ) {
            workspaceReviewPermFilesDto.setCfMstReviewId( permFileShort.getCfMstReviewId().intValue() );
        }
        if ( permFileShort.getCompanyPermFileId() != null ) {
            workspaceReviewPermFilesDto.setCompanyPermFileId( permFileShort.getCompanyPermFileId().toString() );
        }
        workspaceReviewPermFilesDto.setCreatedByName( permFileShort.getCreatedByName() );
        workspaceReviewPermFilesDto.setCreatedDt( permFileShort.getCreatedDt() );
        if ( permFileShort.getDeleted() != null ) {
            workspaceReviewPermFilesDto.setDeleted( Integer.parseInt( permFileShort.getDeleted() ) );
        }
        workspaceReviewPermFilesDto.setExmStsUpdatedByName( permFileShort.getExmStsUpdatedByName() );
        workspaceReviewPermFilesDto.setExmrStatusUpdatedDt( xmlGregorianCalendarToString( dateToXmlGregorianCalendar( permFileShort.getExmrStatusUpdatedDt() ), null ) );
        workspaceReviewPermFilesDto.setExmrTopicStatus( permFileShort.getExmrTopicStatus() );
        if ( permFileShort.getReviewRound() != null ) {
            workspaceReviewPermFilesDto.setReviewRound( permFileShort.getReviewRound().intValue() );
        }
        if ( permFileShort.getRvwPermFileId() != null ) {
            workspaceReviewPermFilesDto.setRvwPermFileId( permFileShort.getRvwPermFileId().intValue() );
        }
        if ( permFileShort.getRvwProfessionId() != null ) {
            workspaceReviewPermFilesDto.setRvwProfessionId( permFileShort.getRvwProfessionId().intValue() );
        }
        workspaceReviewPermFilesDto.setRvwrTopicStatus( permFileShort.getRvwrTopicStatus() );
        workspaceReviewPermFilesDto.setTagFlg( permFileShort.getTagFlg() );
        workspaceReviewPermFilesDto.setTopicApprovedDt( xmlGregorianCalendarToString( dateToXmlGregorianCalendar( permFileShort.getTopicApprovedDt() ), null ) );
        workspaceReviewPermFilesDto.setTopicDescription( permFileShort.getTopicDescription() );
        workspaceReviewPermFilesDto.setTopicName( permFileShort.getTopicName() );
        workspaceReviewPermFilesDto.setTpcApdByName( permFileShort.getTpcApdByName() );
        workspaceReviewPermFilesDto.setUpdatedByName( permFileShort.getUpdatedByName() );

        return workspaceReviewPermFilesDto;
    }

    protected RvwPreliminaryStepsWrkspPK rvwPreliminaryStepsWrkspDtoToRvwPreliminaryStepsWrkspPK(RvwPreliminaryStepsWrkspDto rvwPreliminaryStepsWrkspDto) {
        if ( rvwPreliminaryStepsWrkspDto == null ) {
            return null;
        }

        RvwPreliminaryStepsWrkspPK rvwPreliminaryStepsWrkspPK = new RvwPreliminaryStepsWrkspPK();

        rvwPreliminaryStepsWrkspPK.setRvwProfessionId( rvwPreliminaryStepsWrkspDto.getRvwProfessionId() );
        rvwPreliminaryStepsWrkspPK.setRvwRound( rvwPreliminaryStepsWrkspDto.getRvwRound() );
        rvwPreliminaryStepsWrkspPK.setCfMstReviewId( rvwPreliminaryStepsWrkspDto.getCfMstReviewId() );

        return rvwPreliminaryStepsWrkspPK;
    }
}
